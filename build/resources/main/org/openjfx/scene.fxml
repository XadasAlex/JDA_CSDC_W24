<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Button?>
<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
           prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.openjfx.FXMLController">
    <children>
        <Label fx:id="statusL" layoutX="20" layoutY="20" text="Status:" />
        <Label fx:id="serverListL" layoutX="20" layoutY="50" text="Server List:" />
        <Label fx:id="sessionL" layoutX="20" layoutY="80" text="Logged In:" />
        <ImageView fx:id="profileIV" layoutX="20" layoutY="120" fitHeight="100" fitWidth="100" />
        <Button fx:id="startBTN" text="Start Bot" layoutX="150" layoutY="20" onAction="#startBot" />
        <!--<Label fx:id="serverListIdL" text="Ids: " StackPane.alignment="BOTTOM_LEFT" styleClass="label"/>-->
    </children>
</StackPane>